    <metadata>
        <schema>ADL SCORM</schema>
        <schemaversion>2004 4th Edition</schemaversion>
    </metadata>
    <organizations default="TOC">
        <organization identifier="TOC" adlseq:objectivesGlobalToSystem="false">
            <title>SCO 1</title>

            <!-- SCOs -->
            <item identifier="i1"  identifierref="r1" isvisible="true">
                <title>SCO_1</title>
            </item>
